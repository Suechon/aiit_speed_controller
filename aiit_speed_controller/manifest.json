{
    "manifest_version": 3,
    "name": "AIIT Speed Controller",
    "version": "1.0.0",
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "activeTab",
        "scripting"
    ],
    "host_permissions": [
        "*://va.aiit.ac.jp/*",
        "*://kaf.va.aiit.ac.jp/*"
    ],
    "background": {
        "service_worker": "background.js"
    }
}